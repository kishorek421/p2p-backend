{
  "expo": {
    "name": "go_desk_workplace",
    "slug": "go_desk_workplace",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "godeskworkplace",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.go_desk_workplace.go_desk_workplace",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your photos to upload damaged asset images for reporting purposes.",
          "cameraPermission": "Allow access to your camera to capture photos of damaged assets for reporting purposes."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "godesk_workplace",
          "organization": "bellwether-business-solutions"
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "We would like to track your activity to provide a better experience and personalized content."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "ios": {
      "bundleIdentifier": "com.godesk.godeskworkplace",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow access to your camera to capture photos of damaged assets for reporting purposes",
        "NSUserTrackingUsageDescription": "We would like to track your activity to provide a better experience and personalized content."
      }
    },
    "extra": {
      "eas": {
        "projectId": "2e179dc8-43fd-4ea1-ae65-a5addf9ec14a"
      }
    }
  }
}
